    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "start": "NODE_ENV=production mkdir -p logs && (node src/ | tee -a logs/access.log) 3>&1 1>&2 2>&3 | tee -a logs/error.log"
  },
